{
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "singleAttributePerLine": true,
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(react-dom/(.*)$)|^(react-dom$)",
    "^(react-router-dom/(.*)$)|^(react-router-dom$)",
    "^vitest",
    "^@testing-library/(.*)$",
    "^cypress",
    "<THIRD_PARTY_MODULES>",
    "^types$",
    "^@/types/(.*)$",
    "^@/config/(.*)$",
    "^@/lib/(.*)$",
    "^@/hooks/(.*)$",
    "^@/store/(.*)$",
    "^@/redux/(.*)$",
    "^@/context/(.*)$",
    "^@/http/(.*)$",
    "^@/layouts/(.*)$",
    "^@/pages/(.*)$",
    "^@/hocs/(.*)$",
    "^@/components/ui/(.*)$",
    "^@/components/(.*)$",
    "^@/utils/(.*)$",
    "^@/(constants|data)/(.*)$",
    "^@/styles/(.*)$",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "importOrderTypeScriptVersion": "5.0.2",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
