{
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "singleAttributePerLine": true,
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(react-dom/(.*)$)|^(react-dom$)",
    "^(react-router-dom/(.*)$)|^(react-router-dom$)",
    "^vitest",
    "^@testing-library/(.*)$",
    "^cypress",
    "<THIRD_PARTY_MODULES>",
    "^types$",
    "^@/types/(.*)$",
    "^@/config/(.*)$",
    "^config$",
    "^@/lib/(.*)$",
    "^lib$",
    "^@/hooks/(.*)$",
    "^hooks$",
    "^@/store/(.*)$",
    "^store$",
    "^@/stores/(.*)$",
    "^stores$",
    "^@/redux/(.*)$",
    "^redux$",
    "^@/context/(.*)$",
    "^context$",
    "^@/contexts/(.*)$",
    "^contexts$",
    "^@/http/(.*)$",
    "^http$",
    "^@/api/(.*)$",
    "^api$",
    "^@/actions/(.*)$",
    "^actions$",
    "^@/layouts/(.*)$",
    "^layouts$",
    "^@/pages/(.*)$",
    "^pages$",
    "^@/hocs/(.*)$",
    "^hocs$",
    "^@/components/ui/(.*)$",
    "^components/ui$",
    "^@/components/(.*)$",
    "^components$",
    "^@/utils/(.*)$",
    "^utils$",
    "^@/helpers/(.*)$",
    "^helpers$",
    "^@/(constants|data)/(.*)$",
    "^(constants|data)$",
    "^@/styles/(.*)$",
    "^styles$",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "importOrderTypeScriptVersion": "5.0.2",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
